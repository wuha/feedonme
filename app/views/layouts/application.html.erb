<!DOCTYPE html>
<html>
<head>
  <title>Feedonme</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="message">
	
	<% if flash[:notice]%>
		<%= flash[:notice]%>
	<%end%>
	<% if flash[:alert]%>
		<%= flash[:alert]%>
	<%end%>
	
</div>

<%= yield %>

</body>
</html>
