<ul id="dashboard_subscription_list">
<li id="feed_id_all_unread>" class="subscription_unselected"><%=link_to "all unread", show_feed_unread_entries_path("all"), method: :get, remote: true%><div><%=@user_subscription_all_unread_count%></div></li>
<% @user_subscriptions.each do |subscription|%>
	<li id="feed_id_<%=subscription[:feed_id]%>" class="subscription_unselected"><%= link_to subscription[:name], show_feed_unread_entries_path(subscription[:feed_id]), method: :get, remote: true%><div>[<%=subscription[:unread]%>]</div></li>
<%end%>
</ul>